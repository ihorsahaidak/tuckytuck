{
  "name": "Tucky Tuck",
  "description": "live css editor",
  "icons": {
    "128": "icons/icon128.png",
    "48": "icons/icon48.png",
    "16": "icons/icon16.png"
  },
  "version": "0.0.1",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": [
        "js/main.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "icons/icon16.png",
    "default_popup": "html/popup.html"
  }
}
